CREATE POLICY "Allow authenticated uploads" ON "storage"."buckets"
AS PERMISSIVE FOR INSERT
TO authenticated

WITH CHECK (auth.uid() IS NOT NULL)

CREATE POLICY "Allow public read 1vs8c42_0" ON storage.objects FOR SELECT TO anon, authenticated USING (bucket_id = 'gallery');

CREATE POLICY "Allow authenticated delete" ON "storage"."buckets"
AS PERMISSIVE FOR DELETE
TO authenticated
USING (auth.uid() IS NOT NULL)


